type ProductOrder {
	id: ID!
	order: Order
	product: Product
}

input NewProductOrderInput {
	productId: ID!
	quantity: Int!
}

input EditProductOrderInput {
	id: ID!
	orderId: ID!
	productId: ID!
	quantity: Int!
}

extend type Query {
	findAllProductOrders: [ProductOrder]!
	findAllProductsFromOrdersByProductId(id: ID!): [ProductOrder]!
	findProductsFromOrdersByProductId(id: ID!): [Product]!
}
