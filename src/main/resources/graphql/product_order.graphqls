type ProductOrder {
	id: ID!
	order: Order
	product: Product
	quantity: Int
}

input NewProductOrderInput {
	productId: ID!
	quantity: Int!
}

input EditProductOrderInput {
	id: ID!
	productId: ID!
	quantity: Int!
}

extend type Query {
	findAllProductsFromOrdersByProductId(id: ID!): [ProductOrder]!
}
