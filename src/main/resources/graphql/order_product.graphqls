type OrderProduct {
	id: ID!
	order: Order
	product: Product
}

input OrderProductInput {
	product: ProductInput
	quantity: Int
}

extend type Query {
	findAllOrdersProducts: [OrderProduct]!
	findAllProductsFromOrdersByProductId(id: ID!): [OrderProduct]!
	findProductsFromOrdersByProductId(id: ID!): [Product]!
}
