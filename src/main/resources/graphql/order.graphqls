scalar DateTime

type Order {
	id: ID!
	status: Status
	dateCreated: DateTime
	productOrders: [ProductOrder]
	user: User
}

input OrderInput {
	id: ID
	status: Status
}

enum Status {
	CREATED
	FINISHED
}

extend type Mutation {
	newOrder(orderProducts: [ProductOrderInput]!): Order
	editOrder(id: ID!, orderProducts: [ProductOrderInput]!): Order
}

extend type Query {
	findAllOrders: [Order]!
}
