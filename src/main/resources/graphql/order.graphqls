scalar DateTime

type Order {
	id: ID!
	status: Status
	dateCreated: DateTime
	orderProducts: [OrderProduct]
	user: User
}

input OrderInput {
	id: ID
	status: Status
}

enum Status {
	CREATED
	FINISHED
}

type Mutation {
	newOrder(orderProducts: [OrderProductInput]!): Order
	editOrder(id: ID!, orderProducts: [OrderProductInput]!): Order
}

extend type Query {
	findAllOrders: [Order]!
}
